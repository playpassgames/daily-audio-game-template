{
  "name": "template",
  "private": true,
  "version": "0.0.7",
  "scripts": {
    "start": "vite --open --host",
    "build": "vite build --mode production",
    "test": "eslint src",
    "lint": "eslint --fix src",
    "deploy:staging": "aws s3 sync . s3://template-bucket-staging/daily-music-game/$(jq -r .version ./package.json) --delete --exclude '.github/*' --exclude 'node_modules/*' --exclude '.git/*' --exclude '.idea/*'",
    "deploy": "aws s3 sync . s3://playpass-template-bucket/daily-music-game/$(jq -r .version ./package.json) --delete --exclude '.github/*' --exclude 'node_modules/*' --exclude '.git/*' --exclude '.idea/*'"
  },
  "dependencies": {
    "normalize-url": "^7.0.3",
    "lodash.uniqby": "^4.7.0",
    "playpass": "^1.9.1"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "eslint": "^8.11.0",
    "vite": "^2.8.0"
  }
}
